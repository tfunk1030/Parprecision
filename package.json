{
  "name": "golf-ball-flight-model",
  "version": "1.0.0",
  "description": "Professional-grade golf ball flight simulation model",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:validation": "ts-node src/tests/run-validation-suite.ts",
    "test:all": "npm run test && npm run test:validation",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --out docs src/api/flight-model.ts",
    "benchmark": "ts-node src/benchmark/performance.ts"
  },
  "keywords": [
    "golf",
    "physics",
    "simulation",
    "trajectory",
    "sports"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
